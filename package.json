{
  "name": "youdle",
  "version": "1.1.0-beta",
  "license": "MIT",
  "description": "Desktop Youtube mp3 downloader",
  "homepage": "https://github.com/radiium/youdle",
  "repository": {
    "type": "git",
    "url": "https://github.com/radiium/youdle.git"
  },
  "author": {
    "name": "radiium",
    "email": "remip.dev@gmail.com",
    "url": "https://github.com/radiium"
  },
  "keywords": [
    "electron",
    "angular 7",
    "typescript",
    "ytdl",
    "youtube",
    "youtube downloader",
    "youtube to mp3"
  ],
  "main": "index.js",
  "private": true,
  "scripts": {
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "serve": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "build:electron": "tsc -p src-electron/ && npx copyfiles --flat package.json src-electron/postcss.config.js dist/",
    "build:all": "npm run build:prod && npm run build:electron",
    "start": "npm-run-all -p serve electron:reload",
    "start:web": "npm run serve -- -o",
    "start:prod": "npm run build && npm run build:electron && electron dist/",
    "electron:reload": "npm run build:electron && wait-on http-get://localhost:4200/ && electron dist/ --dev",
    "release:all": "npm run release -- -lmw",
    "release": "npm run build:all && electron-builder build --config electron-builder.yml"
  },
  "dependencies": {
    "electron-context-menu": "^0.10.0",
    "electron-json-storage": "^4.1.3",
    "electron-log": "^2.2.17",
    "fluent-ffmpeg": "^2.1.2",
    "ytdl-core": "^0.29.0",
    "ytpl": "^0.1.14"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.11.4",
    "@angular/animations": "^7.2.0",
    "@angular/cdk": "^7.2.1",
    "@angular/cli": "7.1.4",
    "@angular/common": "7.2.0",
    "@angular/compiler": "7.2.0",
    "@angular/compiler-cli": "7.2.0",
    "@angular/core": "7.2.0",
    "@angular/forms": "7.2.0",
    "@angular/http": "7.2.0",
    "@angular/language-service": "7.2.0",
    "@angular/material": "^7.2.1",
    "@angular/platform-browser": "7.2.0",
    "@angular/platform-browser-dynamic": "7.2.0",
    "@angular/router": "7.2.0",
    "@mdi/angular-material": "^3.3.92",
    "@ngrx/effects": "^7.0.0",
    "@ngrx/entity": "^7.0.0",
    "@ngrx/store": "^7.0.0",
    "@ngrx/store-devtools": "^7.0.0",
    "@types/ffmpeg-static": "^2.0.0",
    "@types/jasmine": "^3.3.5",
    "@types/jasminewd2": "^2.0.5",
    "@types/node": "^10.11.4",
    "codelyzer": "^4.5.0",
    "copyfiles": "^2.1.0",
    "core-js": "^2.5.7",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "^4.0.1",
    "electron-builder": "^20.28.4",
    "electron-icon-maker": "0.0.4",
    "electron-reload": "^1.2.5",
    "ffmpeg-static": "^2.3.0",
    "jasmine-core": "^3.2.1",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "^3.0.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.3.1",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "ncp": "^2.0.0",
    "ngrx-store-freeze": "^0.2.4",
    "ngx-electron": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "npx": "10.2.0",
    "protractor": "^5.4.1",
    "require-dir": "^1.1.0",
    "rxjs": "6.3.3",
