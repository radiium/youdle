{
  "name": "youdle",
  "version": "0.0.1",
  "license": "MIT",
  "description": "Electron angular boilerplate",
  "homepage": "https://github.com/radiium/youdle",
  "repository": {
    "type": "git",
    "url": "https://github.com/radiium/youdle.git"
  },
  "author": {
    "name": "radiium",
    "email": "remip.dev@gmail.com",
    "url": "https://github.com/radiium"
  },
  "keywords": [
    "electron",
    "angular 6",
    "typescript",
    "ytdl",
    "youtube",
    "youtube downloader",
    "youtube to mp3"
  ],
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start:open": "ng serve -o",
    "start:prod": "ng serve --prod",
    "prepare": "ng build && npm run electron:tsc && npm run electron:ncp",
    "prepare:prod": "ng build --prod && npm run electron:tsc && npm run electron:ncp",
    "start:electron": "npm run prepare:dev && electron dist/",
    "start:electron:prod": "npm run prepare:prod && electron dist/",
    "start:electron:reload": "npm-run-all -p start electron:reload",
    "electron:reload": "npm run electron:tsc && npm run electron:ncp && wait-on http-get://localhost:4200/ && electron dist/",
    "electron:tsc": "tsc -p src-electron/",
    "electron:ncp": "ncp src-electron/package.json dist/package.json && ncp src-electron/postcss.config.js dist/postcss.config.js",
    "dist": "electron-builder -lmw --config electron-builder.yml",
    "package:dev": "    npm run prepare:dev  && electron-packager dist --out=release --overwrite",
    "package:prod": "   npm run prepare:prod && electron-packager dist --out=release --asar --overwrite",
    "package": "        npm run package:dev",
    "release": "        npm run package:prod && build",
    "pack": "           electron-builder -lmw --config electron-builder.yml --dir"
  },
  "dependencies": {
    "@angular/animations": "^6.1.9",
    "@angular/cdk": "^6.4.7",
    "@angular/common": "^6.1.9",
    "@angular/compiler": "^6.1.9",
    "@angular/core": "^6.1.9",
    "@angular/forms": "^6.1.9",
    "@angular/http": "^6.1.9",
    "@angular/material": "^6.4.7",
    "@angular/platform-browser": "^6.1.9",
    "@angular/platform-browser-dynamic": "^6.1.9",
    "@angular/router": "^6.1.9",
    "@mdi/font": "^2.8.94",
    "@types/ffmpeg-static": "^2.0.0",
    "async": "^2.6.1",
    "core-js": "^2.5.4",
    "electron-context-menu": "^0.10.0",
    "electron-json-storage": "^4.1.3",
    "ffmpeg-static": "^2.3.0",
    "fluent-ffmpeg": "^2.1.2",
    "fs-extra": "^7.0.0",
    "hammerjs": "^2.0.8",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "ngx-electron": "^1.0.4",
    "npm": "^6.4.1",
    "rxjs": "^6.3.3",
    "ytdl-core": "^0.26.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.8.4",
    "@angular/cli": "~6.2.4",
    "@angular/compiler-cli": "^6.1.9",
    "@types/jasmine": "^2.8.9",
    "@types/jasminewd2": "^2.0.5",
    "@types/node": "^10.11.4",
    "codelyzer": "^4.5.0",
    "electron": "^2.0.7",
    "electron-builder": "^20.28.4",
    "electron-builder-squirrel-windows": "^20.28.3",
    "electron-packager": "^12.2.0",
    "electron-reload": "^1.2.5",
    "jasmine-core": "^3.2.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^1.3.1",
    "ncp": "^2.0.0",
    "npm-run-all": "^4.1.3",
    "protractor": "^5.4.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "~2.9.2",
    "wait-on": "^3.1.0"
  }
}
