{
  "name": "youdle",
  "version": "0.0.1",
  "license": "MIT",
  "description": "Electron angular boilerplate",
  "homepage": "https://github.com/radiium/youdle",
  "repository": {
    "type": "git",
    "url": "https://github.com/radiium/youdle.git"
  },
  "author": {
    "name": "radiium",
    "email": "remip.dev@gmail.com",
    "url": "https://github.com/radiium"
  },
  "keywords": [
    "electron",
    "angular 6",
    "typescript",
    "ytdl",
    "youtube",
    "youtube downloader",
    "youtube to mp3"
  ],
  "main": "electron.js",
  "private": true,
  "scripts": {
    "postinstall": "npm run postinstall:electron && npx electron-builder install-app-deps",
    "postinstall:web": "node postinstall-web",
    "postinstall:electron": "node postinstall",
    "ng": "ng",
    "start": "npm run postinstall:electron && npm-run-all -p ng:serve electron:serve",
    "build": "npm run postinstall:electron && ng build && npm run electron:serve-tsc && npm run electron:ncp",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "ng:serve": "ng serve",
    "electron:ncp": "ncp ./package.json dist/package.json && ncp src-electron/postcss.config.js dist/postcss.config.js",
    "ng:serve:web": "npm run postinstall:web && ng serve -o",
    "electron:serve-tsc": "tsc -p src-electron/ --outDir dist/",
    "electron:serve": "wait-on http-get://localhost:4200/ && npm run electron:serve-tsc && npm run electron:ncp && electron ./dist --dev",
    "electron:local": "npm run build:prod && electron .",
    "electron:linux": "npm run build:prod && npx electron-builder build --linux",
    "electron:windows": "npm run build:prod && npx electron-builder build --windows",
    "electron:mac": "npm run build:prod && npx electron-builder build --mac",
    "test": "npm run postinstall:web && ng test",
    "e2e": "npm run postinstall:web && ng e2e"
  },
  "dependencies": {
    "electron-context-menu": "^0.10.0",
    "electron-json-storage": "^4.1.3",
    "electron-log": "^2.2.17",
    "fluent-ffmpeg": "^2.1.2",
    "ytdl-core": "^0.26.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.8.4",
    "@angular/animations": "^6.1.9",
    "@angular/cdk": "^6.4.7",
    "@angular/cli": "6.2.4",
    "@angular/common": "6.1.9",
    "@angular/compiler": "6.1.9",
    "@angular/compiler-cli": "6.1.9",
    "@angular/core": "6.1.9",
    "@angular/forms": "6.1.9",
    "@angular/http": "6.1.9",
    "@angular/language-service": "6.1.9",
    "@angular/material": "^6.4.7",
    "@angular/platform-browser": "6.1.9",
    "@angular/platform-browser-dynamic": "6.1.9",
    "@angular/router": "6.1.9",
    "@mdi/font": "^2.8.94",
    "@ngrx/store": "^6.1.0",
    "@types/ffmpeg-static": "^2.0.0",
    "@types/jasmine": "^2.8.9",
    "@types/jasminewd2": "^2.0.5",
    "@types/node": "^10.11.4",
    "codelyzer": "^4.5.0",
    "core-js": "^2.5.7",
    "del": "^3.0.0",
    "electron": "^3.0.3",
    "electron-builder": "^20.28.4",
    "electron-reload": "^1.2.5",
    "ffmpeg-static": "^2.3.0",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-run": "^1.7.1",
    "gulp-typescript": "^5.0.0-alpha.3",
    "jasmine-core": "^3.2.1",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "^3.0.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "1.1.2",
    "karma-jasmine-html-reporter": "^1.3.1",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "ncp": "^2.0.0",
    "ngx-electron": "^1.0.4",
    "npm-run-all": "4.1.3",
    "npx": "10.2.0",
    "protractor": "^5.4.1",
    "require-dir": "^1.1.0",
    "rxjs": "6.3.3",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "2.9.2",
    "wait-on": "^3.1.0",
    "webdriver-manager": "^12.1.0",
    "zone.js": "0.8.26"
  }
}
