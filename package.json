{
  "name": "youdle",
  "version": "2.0.1",
  "license": "MIT",
  "description": "Desktop Youtube mp3 downloader",
  "homepage": "https://github.com/radiium/youdle",
  "repository": {
    "type": "git",
    "url": "https://github.com/radiium/youdle.git"
  },
  "author": {
    "name": "radiium",
    "email": "remip.dev@gmail.com",
    "url": "https://github.com/radiium"
  },
  "keywords": [
    "electron",
    "angular 7",
    "typescript",
    "ytdl",
    "youtube",
    "youtube downloader",
    "youtube to mp3"
  ],
  "main": "index.js",
  "private": true,
  "scripts": {
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "serve": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "build:electron": "tsc -p src-electron/ && npx copyfiles --flat package.json src-electron/postcss.config.js dist/",
    "build:all": "npm run build:prod && npm run build:electron",
    "start": "npm-run-all -p serve electron:reload",
    "start:web": "npm run serve -- -o",
    "start:prod": "npm run build && npm run build:electron && electron dist/",
    "electron:reload": "npm run build:electron && wait-on http-get://localhost:4200/ && electron dist/ --dev",
    "release:all": "npm run release -- -lmw",
    "release": "npm run build:all && electron-builder build --config electron-builder.yml"
  },
  "dependencies": {
    "electron-context-menu": "^0.11.0",
    "electron-json-storage": "^4.2.0",
    "electron-log": "^3.0.1",
    "fluent-ffmpeg": "^2.1.2",
    "ytdl-core": "^0.29.1",
    "ytpl": "^0.1.14"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.29",
    "@angular/animations": "^8.2.14",
    "@angular/cdk": "^7.3.2",
    "@angular/cli": "8.3.29",
    "@angular/common": "8.2.14",
    "@angular/compiler": "8.2.14",
    "@angular/compiler-cli": "8.2.14",
    "@angular/core": "8.2.14",
    "@angular/forms": "8.2.14",
    "@angular/language-service": "8.2.14",
    "@angular/material": "^7.3.2",
    "@angular/platform-browser": "8.2.14",
    "@angular/platform-browser-dynamic": "8.2.14",
    "@angular/router": "8.2.14",
    "@mdi/angular-material": "^3.4.93",
    "@ngrx/effects": "^7.2.0",
    "@ngrx/entity": "^7.2.0",
    "@ngrx/store": "^7.2.0",
    "@ngrx/store-devtools": "^7.2.0",
    "@types/ffmpeg-static": "^2.0.0",
    "@types/jasmine": "^3.6.0",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^11.9.4",
    "codelyzer": "^5.0.1",
    "copyfiles": "^2.4.0",
    "core-js": "^2.6.5",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "^10.1.5",
    "electron-builder": "^22.9.1",
    "electron-icon-maker": "0.0.4",
    "electron-reload": "^1.5.0",
    "ffmpeg-static": "^2.3.0",
    "jasmine-core": "^3.2.1",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "^4.0.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.3.1",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "ncp": "^2.0.0",
    "ngrx-store-freeze": "^0.2.4",
    "npm-run-all": "^4.1.5",
    "npx": "^10.2.2",
    "protractor": "^5.4.1",
    "require-dir": "^1.1.0",
    "rxjs": "6.6.3",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.1",
    "typescript": "~3.5.3",
